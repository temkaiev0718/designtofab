        },
        "item-3": {
          "type": "item",
          "settings": {
            "label": "2015",
            "subheading": "",
            "title": "Crafted in Italy",
            "content": "<p>Production takes place in a family-run workshop near Naples, known for its artisanal expertise. The collaboration is close and ongoing, with daily conversations shaping both the process and the products. More than just a manufacturerâ€”true partners in craft.<\/p>",
            "link_text": "",
            "link_url": "",
            "link_style": "link",
            "mobile_text_color": "#ffffff"
          }
        },
        "item-4": {
          "type": "item",
          "settings": {
            "label": "2017",
            "subheading": "Subheading",
            "title": "Discover our products",
            "content": "<p>Located in the heart of the city, our showroom is open throughout the week and offers the ideal place to explore the full range in person.<\/p>",
            "link_text": "",
            "link_url": "",
            "link_style": "link",
            "mobile_text_color": "#ffffff"
          }
        }
      },
